HOST = i686-elf

AR = $(HOST)-ar
AS = $(HOST)-as
CC = $(HOST)-gcc

PREFIX = /usr
EXEC_PREFIX = $(PREFIX)
BOOTDIR = /boot
LIBDIR = $(EXEC_PREFIX)/lib
INCLUDEDIR = $(PREFIX)/include
SYSROOT = $(PWD)/sysroot
BIN_DIR = $(PWD)/bin

CFLAG = --sysroot=$(SYSROOT) -isystem=$(INCLUDEDIR) -O2 -Wall -Wextra -std=gnu11 -ffreestanding
CPPFLAG = 
